    <script>document.documentElement.className = document.documentElement.className.replace('no-js', 'js');</script>
    <script type="text/javascript" src="https://www.bugherd.com/sidebarv2.js?apikey=rsujilyzhudhzkchvo6atg" async="true"></script>
  </head>

  <body class="template-{{ template }}"
    style="
    --background: #F1ECE3;
    --text: #2B1D0A;
    --button: #2B1D0A;
    --button_text: #F1ECE3;
    --nav-height: 0px; 
    --announcement-bar-height: 0px; 
    --footer-height: 0px;"
    >
    <!-- Google Tag Manager (noscript) -->
    <noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-KTC82Q8"
    height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
    <!-- End Google Tag Manager (noscript) -->
    
    <a class="skip-to-content-link button visually-hidden" href="#MainContent">
      {{ "accessibility.skip_to_text" | t }}
    </a>

    <div class="page-overlay fixed below-nav right-0 bottom-0 left-0 bg-black/20 z-50 invisible opacity-0 transition-visibility"></div>

    <div class="fixed w-screen top-0 z-[35] headroom {% if template.name == 'index' %}overlay-header{% endif %}" x-data="{ openMenu: false }" :data-menu-open="openMenu" data-headroom>
      <div style="display:none"> {% sections 'header-group' %} </div>
    </div>

    <div style="display:none"> {% section 'mobile-menu' %} </div>

    {% if template != 'cart' %}
      {% section 'cart-drawer' %}
    {% endif %}

    <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
      {{ content_for_layout }}
    </main>

    {% sections 'footer-group' %}

    <ul hidden>
      <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
    </ul>

    <script>
      window.shopUrl = '{{ shop.url }}';
      window.routes = {
        cart_add_url: '{{ routes.cart_add_url }}',
        cart_change_url: '{{ routes.cart_change_url }}',
        cart_update_url: '{{ routes.cart_update_url }}',
        predictive_search_url: '{{ routes.predictive_search_url }}'
      };

      window.cartStrings = {
        error: `{{ 'sections.cart.cart_error' | t }}`,
        quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}`
      }

      window.variantStrings = {
        addToCart: `{{ 'products.product.add_to_cart' | t }}`,
        soldOut: `{{ 'products.product.sold_out' | t }}`,
        unavailable: `{{ 'products.product.unavailable' | t }}`,
      }

      window.accessibilityStrings = {
        shareSuccess: `{{ 'general.share.success_message' | t }}`,
      }
    </script>
    {% if page.handle == 'email-signup' %}
      <script type="text/javascript">
        document.addEventListener("DOMContentLoaded", (event) => {
          console.log('text')
          window._klOnsite = window._klOnsite || [];
          window._klOnsite.push(['openForm', 'YfziJ3']);  
        });
      </script>
    {% endif %}
  </body>
</html>
