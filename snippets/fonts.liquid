{% style %}
  /* ABC Arizona Flare | Serif - Regular */
  @font-face {
    font-family: arizona-flare;
    src: 
        url("{{ 'ABCArizonaFlare-Regular.woff' | asset_url }}") format("woff"),
        url("{{ 'ABCArizonaFlare-Regular.woff2' | asset_url }}") format("woff2");
    font-weight: 400;
    font-style: normal;
    font-display: block;
  }

  /* ABC Arizona Flare | Serif - Regular Italic */
  @font-face {
    font-family: arizona-flare;
    src: 
        url("{{ 'ABCArizonaFlare-RegularItalic.woff' | asset_url }}") format("woff"),
        url("{{ 'ABCArizonaFlare-RegularItalic.woff2' | asset_url }}") format("woff2");
    font-weight: 400;
    font-style: italic;
    font-display: block;
  }

    /* ABC Oracle | Sans Serif - Regular */
    @font-face {
    font-family: oracle;
    src: 
        url("{{ 'ABCOracle-Book.woff' | asset_url }}") format("woff"),
        url("{{ 'ABCOracle-Book.woff2' | asset_url }}") format("woff2");
    font-weight: 400;
    font-style: normal;
    font-display: block;
  }

  /* ABC Oracle | Sans Serif - Regular Italic */
  @font-face {
    font-family: oracle;
    src: 
        url("{{ 'ABCOracle-BookItalic.woff' | asset_url }}") format("woff"),
        url("{{ 'ABCOracle-BookItalic.woff2' | asset_url }}") format("woff2");
    font-weight: 400;
    font-style: italic;
    font-display: block;
  }

      /* PP Pangram Sans | Sans Serif - SemiBold */
      @font-face {
    font-family: oracle;
    src: 
        url("{{ 'PPPangramSans-Semibold.woff' | asset_url }}") format("woff"),
        url("{{ 'PPPangramSans-Semibold.woff2' | asset_url }}") format("woff2");
    font-weight: 600;
    font-style: normal;
    font-display: block;
  }

{% endstyle %}