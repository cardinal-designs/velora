[
  {
    "name": "theme_info",
    "theme_name": "Custom Theme",
    "theme_version": "1.0.0",
    "theme_author": "Cardinal & Co. Agency",
    "theme_documentation_url": "https:\/\/cardinalco.agency\/",
    "theme_support_url": "https:\/\/cardinalco.agency\/"
  },
  {
    "name": "Logos",
    "settings": [
      {
        "type": "header",
        "content": "Primary"
      },
      {
        "type": "html",
        "id": "logo_svg",
        "label": "SVG"
      },
      {
       "type": "image_picker",
       "id": "logo_image",
       "label": "PNG"
      },
      {
        "type": "header",
        "content": "Secondary"
      },
      {
        "type": "html",
        "id": "secondary_logo",
        "label": "SVG"
      },
    ]
  },
  {
    "name": "Links",
    "settings": [
      {
        "type": "url",
        "id": "shop_all_link",
        "label": "Shop All Link",
        "default": "/collections/all"
      },
      {
        "type": "header",
        "content": "Custom Inquiry Link"
      },
      {
        "type": "text",
        "id": "custom_inquiry_label",
        "label": "Label",
        "default": "Custom Inquiry"
      },
      {
          "type": "url",
          "id": "custom_inquiry_link",
          "label": "Link"
      },
    ]
  },
  {
    "name": "Animations",
    "settings": [
      {
        "type": "checkbox",
        "id": "enable_animations",
        "label": "Enable Animations",
        "default": true
      }
    ]
  },
  {
   "name": "Product",
   "settings": [
    {
      "type": "image_picker",
      "id": "product_image_bg",
      "label": "Product Image Background"
    },
    {
      "type": "checkbox",
      "id": "product_image_multiply",
      "label": "Set image to multiply",
      "default": false
    },
    {
      "type": "paragraph",
      "content": "If the first image for all products has a white background, use this checkbox to allow the texture background to show through"
    }
   ] 
  },
  {
    "name": "Search",
    "settings": [
      {
        "type": "header",
        "content": "Default"
      },
      {
        "type": "collection",
        "id": "search_products",
        "label": "Popular products"
      },
      {
        "type": "textarea",
        "id": "search_suggestions",
        "label": "Popular searchs",
        "info": "Must be a comma-separated lists"
      },
      {
        "type": "header",
        "content": "No Results"
      },
      {
        "type": "text",
        "id": "no_results_text",
        "label": "Text",
        "default": "Nothing found, please try a different search"
      },
      {
        "type": "collection_list",
        "id": "no_results_links",
        "label": "Links"
      }
    ]
  },
  {
    "name": "Cart",
    "settings": [
      {
        "type": "checkbox",
        "id": "enable_shipping_bar",
        "label": "Enable Shipping Bar"
      }
    ]
  },
